{
  "name": "borine",
  "private": true,
  "version": "0.0.0",
  "description": "BORINE monorepo (Expo + React Native)",
  "scripts": {
    "alimi:start": "npm run start --workspace apps/alimi",
    "alimi:android": "npm run android --workspace apps/alimi",
    "alimi:ios": "npm run ios --workspace apps/alimi",
    "alimi:web": "npm run web --workspace apps/alimi",
    "alimi:apk": "node scripts/android-build.mjs alimi apk",
    "alimi:aab": "node scripts/android-build.mjs alimi aab",
    "battery:apk": "node scripts/android-build.mjs battery apk",
    "battery:aab": "node scripts/android-build.mjs battery aab",
    "malhaejwo:apk": "node scripts/android-build.mjs malhaejwo apk",
    "malhaejwo:aab": "node scripts/android-build.mjs malhaejwo aab",
    "malhaejwo:start": "npm run start --workspace apps/malhaejwo",
    "malhaejwo:android": "npm run android --workspace apps/malhaejwo",
    "malhaejwo:ios": "npm run ios --workspace apps/malhaejwo",
    "malhaejwo:web": "npm run web --workspace apps/malhaejwo",
    "memo:apk": "node scripts/android-build.mjs memo apk",
    "memo:aab": "node scripts/android-build.mjs memo aab",
    "lint": "eslint \"apps/**/*.{ts,tsx}\" \"packages/**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "test": "echo \"(????????)\" && exit 0",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.7.4"
  },
  "dependencies": {
    "expo": "~54.0.27",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "expo-notifications": "^0.32.14",
    "expo-application": "^7.0.8",
    "expo-asset": "^12.0.11",
    "expo-font": "^14.0.10",
    "expo-splash-screen": "^31.0.12",
    "react-native-get-random-values": "^2.0.0",
    "react-native-google-mobile-ads": "^16.0.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  }
}
